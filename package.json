{
  "name": "simter-vue-grid",
  "version": "0.1.0",
  "description": "A vue grid with fixed header, group header, cross line merge cell features.",
  "main": "dist/simter-vue-grid.cjs.js",
  "module": "dist/simter-vue-grid.esm.js",
  "browser": "dist/simter-vue-grid.umd.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "~7.5.0",
    "@babel/preset-env": "~7.5.0",
    "rollup": "~1.16.7",
    "rollup-plugin-babel": "~4.3.3",
    "rollup-plugin-commonjs": "~10.0.1",
    "rollup-plugin-json": "~4.0.0",
    "rollup-plugin-node-resolve": "~5.2.0",
    "rollup-plugin-vue": "~5.0.1",
    "uglify-es": "~3.3.9",
    "vue-template-compiler": "~2.6.10"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf temp",
    "build": "yarn clean && rollup -c && yarn min-js",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "min-js": "uglifyjs --comments /^!/ dist/simter-vue-grid.umd.js -o dist/simter-vue-grid.umd.min.js --source-map=url",
    "pretest": "yarn build"
  },
  "files": [
    "src",
    "dist",
    "docs",
    "rollup.config.js"
  ],
  "keywords": [
    "simter",
    "vue",
    "grid",
    "rollup"
  ],
  "repository": "https://github.com/simter-vue/simter-vue-grid.git",
  "author": "RJ.Hwang <rongjihuang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simter-vue/simter-vue-grid/issues"
  },
  "homepage": "https://github.com/simter-vue/simter-vue-grid"
}
